{
  "name": "nuxt-api-shield",
  "version": "0.10.0",
  "description": "Nuxt API Shield - Rate Limiting",
  "repository": "rrd108/nuxt-api-shield",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nuxt",
    "nuxt-module",
    "security",
    "rate-limit",
    "bruteforce"
  ],
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "types": "./dist/index.d.mts"
    },
    "./types": {
      "types": "./dist/index.d.mts"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build && node scripts/generate-types.js",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release:patch": "yarn lint && yarn test && yarn prepack && changelogen --release --patch && yarn publish && git push --follow-tags",
    "release:minor": "yarn lint && yarn test && yarn prepack && changelogen --release --minor && yarn publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --testTimeout 15000 --reporter=default --reporter.summary=false --disable-console-intercept && yarn test:types",
    "test:watch": "vitest watch  --testTimeout 15000 --reporter=default --reporter.summary=false --disable-console-intercept",
    "build:test": "nuxt-module-build build --logLevel silent 2>&1 | grep -v 'Potential missing package.json' || true && node scripts/generate-types.js",
    "test:types": "bash scripts/test-types-fix.sh"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.0",
    "defu": "^6.1.4"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.0.0",
    "@nuxt/eslint-config": "^1.0.0",
    "@nuxt/module-builder": "^1.0.0",
    "@nuxt/schema": "^4.0.0",
    "@nuxt/test-utils": "^3.14.2",
    "@types/node": "latest",
    "changelogen": "^0.6.0",
    "eslint": "^9.10.0",
    "nuxi": "^3.25.1",
    "nuxt": "^4.0.0",
    "typescript": "latest",
    "vitest": "^4.0.0",
    "vue-tsc": "^3.0.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
